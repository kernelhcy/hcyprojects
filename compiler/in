int main()
{	
	int j, m;
	bool con;
	con = false;

	j = 1;
	m = 3;
	n = j + m;
	
	if (j && (k || !m) || !con)
	{
		j = (m + 2) * j;
	}

	while (con)
	{
		j = (j - 1) / n;
		//con = true;
	}

	if (n)
	{
		j = m + (j - 3 * 5) / 2 * i;
	}
	else
	{
		while (j &&(k || !m) && con)
		{
			if ( m || i && ( con && ! ( k || j)))
			{
				j = m + k;
			}
			else
			{
				j = m - k;
			}

		}
	}
	return 0;
}
